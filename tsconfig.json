{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@types/*": [
        "./types/*"
      ],
      "@utils/*": [
        "./utils/*"
      ],
      "@services/*": [
        "./services/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "node_modules/expo-router/types/*.ts"
  ]
}
